{
  "name": "tin-id",
  "version": "2.1.2",
  "description": "connect to TIN service by using node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olustrrax/post-to-tinjs.git"
  },
  "keywords": [
    "TIN",
    "service"
  ],
  "author": "Dusadee Srijulpo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olustrrax/post-to-tinjs/issues"
  },
  "homepage": "https://github.com/olustrrax/post-to-tinjs#readme"
}
